                        <label class="nav navbar-right control-label col-md-1 col-sm-1 col-xs-12" style="margin-top:5px;font-size: 11px;" align="right" for="last-name">
                            Due Date
                        </label>


                        <div class="row" style="margin-top:15px;margin-left:-1%">

                        </div>
                    </div>

                    <div class="x_content">

                        <form id="demo-form2" data-parsley-validate="" class="form-horizontal form-label-left" novalidate="">

                            <input type="hidden" class="custom-control-input" name="Print" value="0" style="width:20px;height:20px;" id="isPrint">
                            <input type="hidden" class="custom-control-input" name="PrintSingleChallan" value="@(ViewData["SingleChallan"] == null ? 0 : 1)" style="width:20px;height:20px;">


                            <div class="form-group">
                                <div class="table-responsive">
                                    <table class="table table-striped table-bordered" id="classTable"  name="model">
                                        <thead>
                                            <tr class="headings">
                                                <th>
                                                </th>
                                                <th>
                                                    <input type="checkbox" id="cbSelectAll" style="width:20px;height:20px;" onclick="CheckAllCheckBoxes();" />
                                                    @if (Model != null && Model.Count > 0)
                                                    {
                                                        <input type="text" value="@Model.Count" id="statusIdAll" style="display: none;" />
                                                    }
                                                    else
                                                    {
                                                        <input type="text" value="0" id="statusIdAll" style="display: none;" />
                                                    }
                                                </th>
                                                <th style="max-width:80px;">
                                                    Issued
                                                </th>
                                                <th style="max-width:80px;">
                                                    Roll No
                                                </th>
                                                <th style="max-width:200px;">
                                                    Name
                                                </th>
                                                <th style="max-width:120px;">
                                                    Contact
                                                </th>
                                                <th style="max-width:200px;">
                                                    Chalan
                                                </th>
                                                <th style="max-width:170px;">
                                                    Due Date
                                                </th>
                                                <th style="max-width:80px;">
                                                    Amount
                                                </th>

                                                <th style="max-width:80px;">
                                                    Fine
                                                </th>
                                                <th style="max-width:80px;">
                                                    Balance
                                                </th>
                                                <th style="max-width:80px;">
                                                    Advance
                                                </th>

                                                <th style="min-width:120px;" hidden="hidden">
                                                    Annual Charges
                                                </th>
                                            </tr>

                                        </thead>
                                        <tbody style="background-color:white;color:#2A3F54" id="classBody">
