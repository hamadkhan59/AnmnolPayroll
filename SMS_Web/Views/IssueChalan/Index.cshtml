</div>

<script>


    function populateErrors()
    {
        @if(ViewData["Error"] != null)
        {
            if ((int)@ViewData["Error"] == 4)
            {
                <text> showNotification('Issue Challan is saved succesfully.', 'Success', 'Success') </text>
            }
            else if ((int)@ViewData["Error"] == 20)
            {
                <text> showNotification('Annual Charges of selected student(s) is reset successfully.', 'Info', 'Info') </text>
            }
            else if ((int)@ViewData["Error"] == 420)
            {
                <text> showNotification('Unable to Save Issued Challan.', 'Error', 'Error') </text>
            }
            else if ((int)@ViewData["Error"] == 5)
            {
                <text> showNotification('Issue Chalan for all selected student(s) to Create Pdf.', 'Info', 'Info') </text>
            }
            else if ((int)@ViewData["Error"] == 6)
            {
                <text> showNotification('Select Student(s) to Issue Chalan.', 'Info', 'Info') </text>
            }
            else if ((int)@ViewData["Error"] == 7)
            {
                <text> showNotification('Select Student(s) to Create Pdf of Issued Chalan(s).', 'Info', 'Info') </text>
            }
            else if ((int)@ViewData["Error"] == 8)
            {
                <text> showNotification('Due Date must be greater than Issued date.', 'Error', 'Error') </text>
            }
            else if ((int)@ViewData["Error"] == 109)
            {
                <text> showNotification('Some of the challans are not issued, Please issue first.', 'Error', 'Error') </text>
            }
            else if ((int)@ViewData["Error"] == 10)
            {
                <text> showNotification('Search Student(s) to Create Pdf of Issue Chalan.', 'Error', 'Error') </text>
            }
            else if ((int)@ViewData["Error"] == 515)
            {
                <text> showNotification('No Fine Settings is defined, please defince settings to Issue Challan.', 'Error', 'Error') </text>
            }
            else if ((int)@ViewData["Error"] == 11)
            {
                <text> showNotification('Search Student(s) to save Issue Chalan.', 'Error', 'Error') </text>
            }
            else if ((int)@ViewData["Error"] == 12)
            {
                <text> showNotification('Please select the correct month to Issue Chalan.', 'Error', 'Error') </text>
            }
            else if ((int)@ViewData["Error"] == 60)
            {
                <text> showNotification('Select Student(s) to Reset Issue Chalan.', 'Error', 'Error') </text>
            }
            else if ((int)@ViewData["Error"] == 4420)
            {
                <text> showNotification('Unable to reset the selected Challan(s).', 'Error', 'Error') </text>
            }
            else if ((int)@ViewData["Error"] == 120)
            {
                <text> showNotification('Some of the Challan(s) are paid, Unpay them to reset the Challan(s)', 'Error', 'Error') </text>
            }
            else if ((int)@ViewData["Error"] == 40)
            {
                <text> showNotification('Selected Student(s) Challan(s) are reset succefully.', 'Success', 'Success') </text>
            }

        }
    }

    function GetNameList() {
        var i = 0;
        @foreach (var item in (IEnumerable<string>)ViewData["nameList"])
        {
              <text>
        var tempObj = '@item';
        var obj = {"AdmissionNo" : tempObj.split(',')[0], "RollNo" : tempObj.split(',')[8], "Name" : tempObj.split(',')[1],
            "FatherName" : tempObj.split(',')[2], "Class" : tempObj.split(',')[6],
            "Section" : tempObj.split(',')[7], "FatherCNIC" : tempObj.split(',')[3],
            "MobileNo" : tempObj.split(',')[4], "Address" : tempObj.split(',')[5]}
        names[i++] = obj;
        </text>
    }
    }

    $(document).ready(function() {
        $('#classTable').DataTable( {
            "lengthMenu": [[-1], ["All"]],
            paging: false, searching : false
        } );
    } );


    var names = [];
    window.onload = function (e) {
        ShowSearchPanel(@((Model == null || Model.Count == 0) ? 0 : Model.Count));
        ChangeAccountType();
        populateErrors();

        $('#dueDate').val('@(DateTime.Now.AddDays(7).ToString("yyyy-MM-dd"))');

        var d = new Date();
        var month = d.getMonth();
        var year = d.getFullYear();

        $("#monthIdSearch").get(0).selectedIndex = month;

        var classId = '@(ViewData["GlobalClassId"] == null ? 0 : (int)ViewData["GlobalClassId"])';
        if (classId != 0) {
            $("#classId").val(classId);
        }
        getSectionList();

        GetNameList();
        setTableData(names);
    }

    function getSectionList() {
        var classId = $("#classId").val();
        if(classId.length == 0)
        {
            classId = 0;
        }
        GetClassSection(classId, 0, 'sectionId', 2, 0);
        //first : classId, seccond : load all option or not, third : section dd html id 
        //fourth : any call function to call, fifth : sectionId to set in DD
    }

    function getChangeSectionList() {
        var classId = $("#classId").val();
        if (classId.length == 0) {
            classId = 0;
        }
        GetClassSection(classId, 0, 'sectionId', 0, 0);
        //first : classId, seccond : load all option or not, third : section dd html id 
        //fourth : any call function to call, fifth : sectionId to set in DD
    }

    function setPrevSearchVals() {
        var sectionId = '@(ViewData["GlobalSectionId"] == null ? 0 : (int)ViewData["GlobalSectionId"])';
        var monthId = '@(ViewData["GlobalMonthId"] == null ? 0 : (int)ViewData["GlobalMonthId"])';
        var yearId = '@(ViewData["GlobalYearId"] == null ? 0 : (int)ViewData["GlobalYearId"])';

        if(sectionId != 0)
        {
            $("#sectionId").val(sectionId);
        }


        if(monthId != 0)
        {
            $("#monthIdSearch").val(monthId);
        }

        if(yearId != 0)
        {
            $("#yearIdSearch").val(yearId);
        }
    }

    function GetChallanDetail(index)
    {
        $("#allownceTable > tbody").html("");

        var rowCount = $('#statusIdAll').val();
        var allownceCount = 0;
        for( var i = 0 ; i < rowCount; i++)
        {
            if($("#tableId"+i).is(':checked') == true || i == index)
            {
                var challanId =  $("#tableId"+i).val();
                $.post( AppDetail() +"/IssueChalan/GetChallanDetail",
                    { challanId: challanId },
                    function (response) {
                        //alert(response[0][0]);
                        for (var i = 0; i < response.length; i++) {

                            var tableRow = '';
                            if(response[i][0].trim() == 'Arrears' || response[i][0].trim() == 'Extra Charges')
                            {
                                tableRow = '<tr style="height:25px;color:red;font-weight:bold;background-color:lightgray;" class="even pointer" id="allRow' + allownceCount + '">' +
                                    '<td>' + response[i][0] + '</td>' +
                                    '<td>' + response[i][1] + '</td>' +
                                    '</tr>';
                            }
                            else if(response[i][0].trim() == 'Total')
                            {
                                tableRow = '<tr id="allRow' + allownceCount + '">' +
                                    '<td style="font-weight:bold;color:black;">' + response[i][0] + '</td>' +
                                    '<td style="font-weight:bold;color:black;">' + response[i][1] + '</td>' +
                                    '</tr>';
                            }
                            else
                            {
                                tableRow = '<tr class="even pointer" id="allRow' + allownceCount + '">' +
                                   '<td>' + response[i][0] + '</td>' +
                                   '<td>' + response[i][1] + '</td>' +
                                   '</tr>';
                            }

                            $("#allownceTable").find('tbody').append(tableRow);
                            allownceCount++;
                        }
                    }
                 );

                break;
            }
        }

    }

   
    function setPrint(value)
    {
        $("#isPrint").val(value);

        if(value == 2)
        {
            ConfirmAction('Are you sure, you want to reset the Issued challans?', 'resetChallan');
            return false;
        }

        return true;
    }

    function CheckFinanceAccount()
    {
        var accountType = $("#accountTypeId option:selected").text();
        var accountCount = $("#financeAccountId").children('option').length;

        if(Number(accountCount) == 1)
        {
            showNotification('No Pay To ' +accountType+ ' Account is opened, Please open the Finance Account then Issue Challan', 'Error', 'Error')
            return false;
        }
        else
        {
            ConfirmAction('Are you sure, you want to Issue the challans?', 'saveChallan');
        }

        return false;
    }

    function RouteToFinanceAccount()
    {
        var accountType = $("#accountTypeId").val();
        if(accountType.trim() == 1)
        {
            window.location.href = AppDetail() + "/BankAccount/Index";
        }
        else
        {
            window.location.href = AppDetail() + "/CashAccount/Index";
        }
    }

   

    function CheckAllCheckBoxes()
    {
        var rowCount = $('#statusIdAll').val();
        if($("#cbSelectAll").is(':checked') == true)
        {
            for( var i = 0 ; i < rowCount; i++)
            {
                SelectElement("tableId"+i, true);
            }
        }
        else
        {
            for( var i = 0 ; i < rowCount; i++)
            {
                SelectElement("tableId"+i, false);
            }
        }
    }

    function SelectElement(id, valueToSelect)
    {
        var element = document.getElementById(id);
        element.checked  = valueToSelect;
    }

    function ChangeAccountType()
    {
        var accountTypeId = $("#accountTypeId").val();
        var branchId = $("#BranchId").val();
        if(accountTypeId == 1)
        {
            accountTypeId = 13;
        }
        else
        {
            accountTypeId = 14;
        }
        $.ajax({
            type: "GET",
            url: AppDetail() + '/api/Common/getFinanceAccount?typeId=' + accountTypeId + '&&branchId=' + branchId + '&&isLoadAll=1',
            contentType: 'application/json',
            success: function (result) {
                $("#financeAccountId").html(result);
            },
            error: function (res) {
                showNotification('Unable to load account', 'Error', 'Error')
            }
        });
    }

</script>
