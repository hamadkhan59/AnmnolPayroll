                                                    <tr>
                                                        <td>
                                                            <button type="button" class="btn btn-xs btn-info btn-squar" onclick="GetChallanDetail(@i)" data-toggle="modal" data-target=".bs-example-modal-sm">Detail</button>
                                                        </td>
                                                        <td>
                                                            <input type="checkbox" class="custom-control-input" name="ChalanIds" value="@Model[i].Id" style="width:20px;height:20px;" id="@("tableId" + i)">
                                                        </td>
                                                        <td style="max-width:80px;">
                                                            @if (Model[i].IsPaid == "Yes")
