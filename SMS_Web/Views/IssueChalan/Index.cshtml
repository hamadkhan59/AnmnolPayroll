                </div>

                <div class="col-md-9 col-sm-9 col-xs-12 nav navbar-right" style="margin-top:2px;">
                    @using (Html.BeginForm(Html.BeginForm("SearchByAdmissionNo", "IssueChalan", FormMethod.Post, new { id = "fastPayForm", enctype = "multipart/form-data" })))
