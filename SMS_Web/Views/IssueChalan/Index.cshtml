                                            <input type="submit" value="Reset the Challan(s)" align="right" onclick="return setPrint(2)" class="btn btn-squar"  id="resetChallan" />
                                        }
                                        <input type="submit" value="Create Pdf" align="right" onclick="return setPrint(1)" class="btn btn-squar"  />
                                        @if (ViewData["SingleChallan"] == null)
                                        {
                                            <input type="submit" value="Save" onclick="return CheckFinanceAccount()" align="right" class="btn btn-squar" id="saveChallan"  />
                                        }
                                    </div>


                                </div>
                            </div>
                        </form>
                    </div>
                </div>

            </fieldset>

        }
    </div>
</div>


<div class="modal fade bs-example-modal-sm" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-sm sc_panel_squar" style="width:600px;margin-top:8%;">
        <div class="modal-content sc_panel_squar">
            <div class="modal-header" style="background-color:#151F2A;">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true" style="color:white;">×</span>
                </button>
                <h4 class="modal-title" id="myModalLabel2" style="color:white;font-weight:bold">Fee Challan Detail</h4>
            </div>
            <div class="modal-body">
                <div class="table-responsive">
                    <table class="table table-striped table-bordered" id="allownceTable" name="model">
                        <thead style="color:black">
                            <tr class="headings">

                                <th style="min-width:80px;">
                                    Fee Head
                                </th>
                                <th style="min-width:100px;">
                                    Amount
